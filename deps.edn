           com.taoensso/encore        {:mvn/version "2.120.0"}
           com.fulcrologic/guardrails {:mvn/version "1.1.11"}
           com.fulcrologic/fulcro-rad {:mvn/version "1.3.10"}
           vvvvalvalval/datomock      {:mvn/version "0.2.2"}
