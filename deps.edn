                                org.clojure/test.check    {:mvn/version "1.1.1"}
                                lambdaisland/kaocha       {:mvn/version "1.69.1069"}
                                com.datomic/local         {:mvn/version "1.0.267"}
                                com.datomic/client-cloud  {:mvn/version "1.0.123"}
